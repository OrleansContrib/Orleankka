{
    "metadata": [{
        "src": [{
            "files": ["**/*.csproj"],
            "exclude": [
                "**/obj/**",
                "**/bin/**",
                "_site/**"
            ],
            "src": "../Source"
        }],
        "dest": "api",
        "filter": "filterConfig.yml",
        "properties": {
            "TargetFramework": "net7.0"
        }
    }],
    "build": {
        "content": [{
            "files": [
                "api/**.yml",
                "api/index.md"
            ]
        }, {
            "files": [
                "articles/**.md",
                "articles/**/toc.yml",
                "toc.yml",
                "*.md"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "resource": [{
            "files": [
                "images/**"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "xref": [
            "../packages/msdn.4.5.2/0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"
        ],
        "globalMetadata": {
            "_appTitle": "Orleankka",
            "_appLogoPath": "images/Logo.png",
            "_appFaviconPath": "/images/favicon.ico",
            "_appFooter": "Copyright Â© 2016-2018 Orleankka project<br>Generated by <strong>DocFX</strong>",
            "_enableSearch": "true"
        },
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "template"
        ],
        "postProcessors": [],
        "noLangKeyword": false
    }
}
